.son-node{
    height:90px;
    background:#044694;
    text-align: center;
    font-size:16px;
    line-height:90px;
    position: relative;
    margin-top:-40px;color:#fff;
    font-weight:bold;
}
.son-node span{
    margin:0px 50px;
    position: relative;
}
.son-node span a{
    color:#fff;
}
.son-node #CurrentlyNode:after{
    content: "";
    position: absolute;
    background: #fff;
    height: 1px;
    width: 100%;
    display: inline-block;
    top: 22px;
    left: 0px;
}

.column1{
    padding-left:262px;
    position: relative;
    min-height:500px;
}
.node-nav{
    width:232px;
    position: absolute;
    top:0px;
    z-index:11;
    margin-left:-248px;
    box-shadow:1px 1px 3px rgba(0,0,0,0.2);
    padding:20px 10px 20px 20px;
}
.node-nav .hd{
    line-height:40px;
    font-size:16px;
    padding:8px 0px;
}
.node-nav .hd a{
    color:#282828;
    font-weight: bold;
    background:url(../img/list06.png) left center no-repeat;
    padding-left:20px;
    cursor: pointer;
}
.node-nav .hd.active a{
    background:url(../img/list07.png) left center no-repeat;
    color:#044694;
}
.node-nav .hd.first a{
    background:none;
}
.first a{
    color:#044694 !important;
}
