.Spinner{display:block;overflow:hidden;width:156px;margin:17px auto 15px auto;}
.Spinner .Amount{width:74px;margin:0px 8px;height:26px;line-height:26px;border:1px solid #e2e2e2;float:left;text-align:center;color:#565656;outline:0;}
.Spinner a{line-height:22px;display:inline-block;width:31px;height:26px;border:1px solid #e2e2e2;background:#fff;float:left;cursor:pointer;outline:0;}
.Spinner a i{font-style:normal;display:block;text-align:center;font-size:22px;color:#818181;overflow:hidden;}

