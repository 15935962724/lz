<%
StringBuilder sb=new StringBuilder();
DbAdapter db=new DbAdapter();
try
{
  db.executeUpdate("UPDATE Donation SET province='' WHERE province IS NULL");
  db.executeUpdate("UPDATE Donation SET city='' WHERE city IS NULL");
  db.executeQuery("SELECT province,COUNT(province) FROM Donation GROUP BY province ORDER BY COUNT(province) DESC");
  while(db.next())
  {
    String pro=db.getString(1);
    int pv=Donation.count(" AND province="+DbAdapter.cite(pro)+" AND city=''");
    int sum=db.getInt(2)-pv;
    if(pro.length()<1)pro="<无>";
    out.print("<tr><td>"+pro+"<td>"+sum+"</td><td>"+pv+"</td><td><a href='/jsp/type/women/Donations.jsp?province="+Http.enc(pro)+"'>详细</a></td></tr>");
    sb.append("var c=$('_C"+pro+"');if(c)c.setAttribute('altx','　地　区："+pro+"<br>　已落实："+sum+"<br>　未落实："+pv+"');");
  }
}finally
{
  db.close();
}
