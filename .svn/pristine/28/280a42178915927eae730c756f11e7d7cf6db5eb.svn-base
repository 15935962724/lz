	  <td><input type="text" name="froms"><script>document.write("<input type=button value=... onclick=showCalendar('form1.froms'); >");</script>
	    <input type="text" name="tos"><script>document.write("<input type=button value=... onclick=showCalendar('form1.tos'); >");</script></td>
	    	  
