<!doctype html>
<html>
  <head>
    <title>leftup</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<script src="/tea/tea.js" type="text/javascript"></script>
        <style>
		body		{
	text-align: left;
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px; overflow:hidden
}
        </style>
<script type="">
var f=parent.document.getElementById("frame2");
function fclick()
{
  var value=getCookie('admin_leftup_hide');
  if(value=='true')
  {
    f.cols='250,10px,*';
    img.src="/tea/image/public/ico_min2.gif";
    setCookie('admin_leftup_hide','false');
  }else
  {
    f.cols='0,10px,*';
    img.src="/tea/image/public/ico_max2.gif";
    setCookie('admin_leftup_hide','true');
  }
  size_button.focus();
}
function fload()
{
  var value=getCookie('admin_leftup_hide');
  var arr=f.cols.split(",");
  if(value=='true')
  {
    f.cols="0,"+arr[1]+","+arr[2]+","+arr[3];
    img.src="/tea/image/public/ico_max2.gif";
  }else
  {
    f.cols="250,"+arr[1]+","+arr[2]+","+arr[3];
    img.src="/tea/image/public/ico_min2.gif";
  }
  size_button.focus();
}
</script>
</head>
<body onLoad="fload();">
<table  border="0"  height="100%" cellpadding="0" cellspacing="0">
  <tr>
    <td valign="middle"><A href="javascript:;" name="size_button" onClick="fclick();"><img name="img" border="0" valign="middle" style=" cursor:hand " src="/tea/image/public/ico_min2.gif" ></A>
</td>
  </tr>
</table>

</body>
</html>




